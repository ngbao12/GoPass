{
    "name": "gopass",
    "private": true,
    "scripts": {
        "dev": "concurrently \"npm run dev:frontend\" \"npm run dev:backend\"",
        "dev:frontend": "cd frontend && npm run dev",
        "dev:backend": "cd backend && npm run dev",
        "install:all": "cd frontend && npm install && cd ../backend && npm install"
    },
    "devDependencies": {
        "nodemon": "^3.1.10",
        "vite": "^7.0.4",
        "concurrently": "^8.2.2"
    }
}